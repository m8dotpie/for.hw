<?xml version="1.0"?>


<robot name="hw1">
  
  <link name="ground_frame">
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
  </link>

  <link name="base_link">
    <visual>
    <origin xyz="0.0 0.0 0.25" rpy="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.1 0.1 0.5"/>
      </geometry>
    </visual>
  </link>

  <link name="base_link1">
    <visual>
    <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.5 0.1 0.1"/>
      </geometry>
    </visual>
  </link>

  <link name="base_link2">
    <visual>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <cylinder length="0.2" radius="0.1"/>
      </geometry>
    </visual>
  </link>

  <link name="base_link3">
    <visual>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 1.57 0.0"/>
      <geometry>
        <cylinder length="0.3" radius="0.1"/>
      </geometry>
    </visual>
  </link>

  <link name="base_link4">
    <visual>
    <origin xyz="0 0.0 0.0" rpy="1.57 0.0 0.0"/>
      <geometry>
        <cylinder length="0.2" radius="0.1"/>
      </geometry>
    </visual>
  </link>

  <joint name="translatory1" type="prismatic">
    <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="30" velocity="1.0" lower="-0.3" upper="0.3"/>	
    <parent link="ground_frame"/>
    <child link="base_link"/>
  </joint>

  <joint name="translatory2" type="prismatic">
    <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0"/>
    <axis xyz="1 0 0"/>
    <limit effort="30" velocity="1.0" lower="-0.20" upper="0.20"/>	
    <parent link="base_link"/>
    <child link="base_link1"/>
  </joint>
	  
  <joint name="rev1" type="revolute">
    <origin xyz="0.35 0.00 0.05" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="30" velocity="1.0" lower="-3.14" upper="3.14"/>	
    <parent link="base_link1"/>
    <child link="base_link2"/>
  </joint>
	  
  <joint name="rev2" type="revolute">
    <origin xyz="0.0 0.0 0.2" rpy="0.0 0.0 0.0"/>
    <axis xyz="1 0 0"/>
    <limit effort="30" velocity="1.0" lower="-3.14" upper="3.14"/>	
    <parent link="base_link2"/>
    <child link="base_link3"/>
  </joint>
	  
  <joint name="rev3" type="revolute">
    <origin xyz="0.25 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 1 0"/>
    <limit effort="30" velocity="1.0" lower="-3.14" upper="3.14"/>	
    <parent link="base_link3"/>
    <child link="base_link4"/>
  </joint>
</robot>
